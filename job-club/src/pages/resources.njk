            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"/><circle cx="8.5" cy="8.5" r="1.5"/><polyline points="21 15 16 10 5 21"/></svg>
          </span>
          <span class="resource-quick-text">
            <strong>Canva</strong>
            <small>Create graphics & mockups</small>
          </span>
        </a>
        <a href="https://fonts.google.com/" target="_blank" rel="noopener" class="resource-quick-link">
          <span class="resource-quick-icon-svg">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><polyline points="4 7 4 4 20 4 20 7"/><line x1="9" y1="20" x2="15" y2="20"/><line x1="12" y1="4" x2="12" y2="20"/></svg>
          </span>
          <span class="resource-quick-text">
            <strong>Google Fonts</strong>
            <small>Free professional fonts</small>
          </span>
        </a>
      </div>
    </div>
  </div>
</section>

<!-- CTA -->
<section class="page-section">
  <div class="container">
    <div class="section-header center">
      <span class="section-eyebrow">Have a Resource?</span>
      <h2 class="section-title">Contribute to the Community</h2>
      <p class="section-subtitle">Know a great resource that should be here? Let us know!</p>
      <div style="margin-top: 2rem;">
        <a href="/join/" class="btn btn-primary btn-large">Join & Suggest â†’</a>
      </div>
    </div>
  </div>
</section>

<!-- Resource Modal -->
<div class="resource-modal" id="resource-modal" aria-hidden="true">
  <div class="modal-backdrop"></div>
  <div class="modal-container" role="dialog" aria-modal="true" aria-labelledby="modal-title">
    <button class="modal-close" aria-label="Close modal">
      <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
        <line x1="18" y1="6" x2="6" y2="18"/>
        <line x1="6" y1="6" x2="18" y2="18"/>
      </svg>
    </button>
    
    <div class="modal-content">
      <div class="modal-header">
        <span class="modal-category" id="modal-category"></span>
        <h2 class="modal-title" id="modal-title"></h2>
        <p class="modal-description" id="modal-description"></p>
      </div>
      
      <div class="modal-body" id="modal-body">
        <!-- Content sections will be injected here -->
      </div>
      
      <div class="modal-footer">
        <a href="#" class="btn btn-primary btn-glow" id="modal-action" target="_blank" rel="noopener">
          Take Action
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6"/>
            <polyline points="15 3 21 3 21 9"/>
            <line x1="10" y1="14" x2="21" y2="3"/>
          </svg>
        </a>
        <button class="btn btn-secondary modal-close-btn">Close</button>
      </div>
    </div>
  </div>
</div>

<!-- Resource Data for JavaScript -->
<script id="resources-data" type="application/json">
{{ resources | dump | safe }}
</script>

<script src="/js/resource-modal.js"></script>
