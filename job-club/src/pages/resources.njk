---
layout: layouts/base.njk
title: Resources
description: Curated resources to help you build your AI career - tutorials, tools, templates, and more.
permalink: /resources/
pageId: resources
---

<section class="page-section">
  <div class="container">
    <div class="section-header">
      <span class="section-eyebrow">Learn & Grow</span>
      <h1 class="section-title">Resources</h1>
      <p class="section-subtitle">Curated resources to help you build your AI career - tutorials, tools, templates, and more.</p>
    </div>
    
    <div class="grid grid-3">
      {% for resource in resources %}
      {% set isExternal = resource.type == 'external' or resource.externalLink or (resource.url and not resource.url.startsWith('/')) %}
      
      {% if isExternal %}
      {# External Resource - Link directly #}
      <a href="{{ resource.url or resource.externalLink }}" class="resource-card" data-type="external" target="_blank" rel="noopener" data-track-event="resource_click" data-track-label="{{ resource.title }}">
      {% else %}
      {# Internal Resource - Opens modal #}
      <div class="resource-card" data-type="internal" data-resource-slug="{{ resource.slug }}" tabindex="0" role="button" aria-label="View {{ resource.title }} guide">
      {% endif %}
      
        <span class="resource-category">{{ resource.category }}</span>
        
        <div class="resource-icon">
          {% if resource.category == "Professional Presence" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <rect x="2" y="7" width="20" height="14" rx="2"/>
              <path d="M16 21V5a2 2 0 0 0-2-2h-4a2 2 0 0 0-2 2v16"/>
            </svg>
          {% elif resource.category == "Portfolio" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <rect x="3" y="3" width="18" height="18" rx="2"/>
              <circle cx="8.5" cy="8.5" r="1.5"/>
              <polyline points="21 15 16 10 5 21"/>
            </svg>
          {% elif resource.category == "Career" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M22 12h-4l-3 9L9 3l-3 9H2"/>
            </svg>
          {% elif resource.category == "AI Tools" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M12 2L2 7l10 5 10-5-10-5z"/>
              <path d="M2 17l10 5 10-5"/>
              <path d="M2 12l10 5 10-5"/>
            </svg>
          {% elif resource.category == "Learning" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M22 10v6M2 10l10-5 10 5-10 5z"/>
              <path d="M6 12v5c3 3 9 3 12 0v-5"/>
            </svg>
          {% elif resource.category == "Tools" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M14.7 6.3a1 1 0 0 0 0 1.4l1.6 1.6a1 1 0 0 0 1.4 0l3.77-3.77a6 6 0 0 1-7.94 7.94l-6.91 6.91a2.12 2.12 0 0 1-3-3l6.91-6.91a6 6 0 0 1 7.94-7.94l-3.76 3.76z"/>
            </svg>
          {% elif resource.category == "Templates" %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z"/>
              <polyline points="14 2 14 8 20 8"/>
              <line x1="16" y1="13" x2="8" y2="13"/>
              <line x1="16" y1="17" x2="8" y2="17"/>
            </svg>
          {% else %}
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"/>
            </svg>
          {% endif %}
        </div>
        
        <h3 class="resource-title">{{ resource.title }}</h3>
        <p class="resource-description">{{ resource.description }}</p>
        
        <span class="resource-link">
          {% if isExternal %}
          Open Resource
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6"/>
            <polyline points="15 3 21 3 21 9"/>
            <line x1="10" y1="14" x2="21" y2="3"/>
          </svg>
          {% else %}
          View Guide
          <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
            <path d="M5 12h14M12 5l7 7-7 7"/>
          </svg>
          {% endif %}
        </span>
        
      {% if isExternal %}
      </a>
      {% else %}
      </div>
      {% endif %}
      {% endfor %}
    </div>
  </div>
</section>

<!-- Resource Categories -->
<section class="page-section">
