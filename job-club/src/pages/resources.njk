<section class="page-section">
  <div class="container">
    <div class="section-header">
      <span class="section-eyebrow">Learn & Grow</span>
      <h1 class="section-title">Resources</h1>
      <p class="section-subtitle">Curated resources to help you build your AI career - tutorials, tools, templates, and more.</p>
    </div>
    
    <div class="grid grid-3">
      {% for resource in resources %}
      <div class="resource-card">
        <span class="resource-category">{{ resource.category }}</span>
        <h3 class="resource-title">{{ resource.title }}</h3>
        <p class="resource-description">{{ resource.description }}</p>
        {% set isInternal = resource.url and resource.url.startsWith('/') %}
        <a href="{{ resource.url }}" class="resource-link" data-track-event="resource_click" data-track-label="{{ resource.title }}" {% if not isInternal %}target="_blank" rel="noopener"{% endif %}>
          View Resource â†’
        </a>
      </div>
      {% endfor %}
    </div>
  </div>
</section>

