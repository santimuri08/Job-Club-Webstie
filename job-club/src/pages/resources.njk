        <h4 class="checklist-category-title">
          <span class="checklist-icon-svg">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 11.08V12a10 10 0 1 1-5.93-9.14"/><polyline points="22 4 12 14.01 9 11.01"/></svg>
          </span>
          Technical Quality
        </h4>
        <ul class="checklist-items">
          <li class="checklist-item">
            <span class="checkbox-icon"><svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"/></svg></span>
            <span class="item-text">Mobile responsive</span>
          </li>
          <li class="checklist-item">
            <span class="checkbox-icon"><svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"/></svg></span>
            <span class="item-text">Fast loading (&lt;3 seconds)</span>
          </li>
          <li class="checklist-item">
            <span class="checkbox-icon"><svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"/></svg></span>
            <span class="item-text">No broken links</span>
          </li>
          <li class="checklist-item">
            <span class="checkbox-icon"><svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><rect x="3" y="3" width="18" height="18" rx="2"/></svg></span>
            <span class="item-text">HTTPS enabled</span>
          </li>
        </ul>
      </div>
    </div>
    
    <!-- Quick Start Resources -->
    <div class="portfolio-resources">
      <h3 class="resources-title">
        <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"><path d="M22 12h-4l-3 9L9 3l-3 9H2"/></svg>
        Quick Start Resources
